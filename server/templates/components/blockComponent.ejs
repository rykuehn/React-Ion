import React from 'react';
<% if (initial) { %>import ReactDOM from 'react-dom'<% } %>
<% for(var i=0; i<children.length; i++) { %>import <%= children[i].name %> from '../components/<%= children[i].name %>';
<% } %>
require('../css/<%= name %>.css');

class <%- name %> extends React.Component {
  render() {

  	return (
  	  <div className="<%= name.toLowerCase() %>-<%= componentType.toLowerCase() %>" >
  	  <% for(var i=0; i<children.length; i++) { %>  <%- children[i].codeString %>
  	  <% } %></div>
  	)
  }

};
<% if (initial) { %>
ReactDOM.render(<<%- name %> />, document.getElementById('App'));
<% } else { %>
export default <%- name %>;
<% } %>